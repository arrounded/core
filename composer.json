{
    "name": "arrounded/core",
    "description": "Core helpers for Laravel projects",
    "license": "MIT",
    "keywords": [
        "laravel"
    ],
    "authors": [
        {
            "name": "Madewithlove",
            "email": "heroes@madewithlove.be"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/contracts": "^5.2",
        "illuminate/support": "^5.2",
        "symfony/http-foundation": "^3.0"
    },
    "require-dev": {
        "fabpot/php-cs-fixer": "2.0.*@dev",
        "illuminate/container": "^5.2",
        "illuminate/routing": "^5.2",
        "mockery/mockery": "^0.9.4",
        "phpunit/phpunit": "^5.2",
        "symfony/var-dumper": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Arrounded\\Core\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Arrounded\\Core\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "lint": "php-cs-fixer fix"
    }
}
